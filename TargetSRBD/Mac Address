#include<iostream>
#include<climits>
#include<cmath>
#include<cstdio>
#include<algorithm>
#include<vector>
using namespace std;
int main(){
    char *mac_add = "ab:cd:ef:12:34:56";
    char result[strlen(mac_add)];
    for(int i=0;i<=strlen(mac_add);i++){
      if(mac_add[i]!=':') result[i]=mac_add[i];
      else result[i] = ',';
    }
    cout<<result<<"\n";
}
